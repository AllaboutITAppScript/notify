<!DOCTYPE html>
<html lang="th">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Web App Notification System</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <link rel="manifest" href="/manifest.json">
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }
        
        body {
            background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);
            color: #333;
            min-height: 100vh;
            padding: 20px;
        }
        
        .container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 20px;
        }
        
        header {
            text-align: center;
            margin-bottom: 30px;
            color: white;
        }
        
        header h1 {
            font-size: 2.5rem;
            margin-bottom: 10px;
            text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.2);
        }
        
        header p {
            font-size: 1.2rem;
            opacity: 0.9;
        }
        
        .notification-container {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 20px;
            margin-bottom: 30px;
        }
        
        .card {
            background-color: white;
            border-radius: 15px;
            padding: 25px;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.15);
            transition: transform 0.3s ease;
        }
        
        .card:hover {
            transform: translateY(-5px);
        }
        
        .card h2 {
            color: #2575fc;
            margin-bottom: 15px;
            font-size: 1.5rem;
            display: flex;
            align-items: center;
            gap: 10px;
        }
        
        .card h2 i {
            background: #2575fc;
            color: white;
            width: 36px;
            height: 36px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
        }
        
        .permission-status {
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 15px;
            background: #f8f9fa;
            border-radius: 10px;
            margin-bottom: 20px;
        }
        
        .status-indicator {
            width: 20px;
            height: 20px;
            border-radius: 50%;
            margin-right: 10px;
        }
        
        .granted { background-color: #4CAF50; }
        .denied { background-color: #f44336; }
        .default { background-color: #ff9800; }
        
        .btn {
            padding: 12px 25px;
            border: none;
            border-radius: 8px;
            font-weight: 600;
            cursor: pointer;
            transition: all 0.3s ease;
            font-size: 1rem;
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 8px;
        }
        
        .btn-primary {
            background: #2575fc;
            color: white;
        }
        
        .btn-primary:hover {
            background: #1a68e8;
            box-shadow: 0 5px 15px rgba(37, 117, 252, 0.4);
        }
        
        .btn-secondary {
            background: #6c757d;
            color: white;
        }
        
        .btn-secondary:hover {
            background: #5a6268;
        }
        
        .btn-success {
            background: #4CAF50;
            color: white;
        }
        
        .btn-success:hover {
            background: #45a049;
        }
        
        .btn-danger {
            background: #f44336;
            color: white;
        }
        
        .btn-danger:hover {
            background: #d32f2f;
        }
        
        .btn-group {
            display: flex;
            flex-wrap: wrap;
            gap: 10px;
            margin-top: 15px;
        }
        
        .notification-list {
            margin-top: 20px;
            max-height: 300px;
            overflow-y: auto;
        }
        
        .notification-item {
            padding: 15px;
            border-radius: 10px;
            margin-bottom: 10px;
            display: flex;
            align-items: center;
            gap: 15px;
            animation: fadeIn 0.5s ease;
            border-left: 5px solid #2575fc;
            background: #f8f9fa;
        }
        
        .notification-item.unread {
            background: #e3f2fd;
            border-left-color: #4CAF50;
        }
        
        .notification-icon {
            width: 40px;
            height: 40px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            color: white;
            font-size: 1.2rem;
        }
        
        .info { background: #2196F3; }
        .success { background: #4CAF50; }
        .warning { background: #ff9800; }
        .error { background: #f44336; }
        
        .notification-content h4 {
            margin-bottom: 5px;
            color: #333;
        }
        
        .notification-content p {
            color: #666;
            font-size: 0.9rem;
        }
        
        .notification-time {
            font-size: 0.8rem;
            color: #999;
            margin-top: 5px;
        }
        
        @keyframes fadeIn {
            from { opacity: 0; transform: translateY(10px); }
            to { opacity: 1; transform: translateY(0); }
        }
        
        @keyframes slideIn {
            from { transform: translateX(100%); opacity: 0; }
            to { transform: translateX(0); opacity: 1; }
        }
        
        .toast-notification {
            position: fixed;
            top: 20px;
            right: 20px;
            width: 350px;
            background: white;
            border-radius: 10px;
            box-shadow: 0 5px 20px rgba(0, 0, 0, 0.15);
            padding: 15px;
            display: flex;
            align-items: center;
            gap: 15px;
            z-index: 1000;
            animation: slideIn 0.5s ease;
            border-left: 5px solid #4CAF50;
        }
        
        .toast-notification.error {
            border-left-color: #f44336;
        }
        
        .toast-notification.warning {
            border-left-color: #ff9800;
        }
        
        .toast-notification.info {
            border-left-color: #2196F3;
        }
        
        .close-toast {
            margin-left: auto;
            background: transparent;
            border: none;
            font-size: 1.2rem;
            cursor: pointer;
            color: #999;
        }
        
        footer {
            text-align: center;
            color: white;
            margin-top: 30px;
            padding-top: 20px;
            border-top: 1px solid rgba(255, 255, 255, 0.1);
        }
        
        @media (max-width: 768px) {
            .container {
                padding: 10px;
            }
            
            .notification-container {
                grid-template-columns: 1fr;
            }
            
            .btn-group {
                flex-direction: column;
            }
            
            .btn {
                width: 100%;
            }
            
            .toast-notification {
                width: calc(100% - 40px);
            }
        }
    </style>
</head>
<body>
    <div class="container">
        <header>
            <h1><i class="fas fa-bell"></i> Web App Notification System</h1>
            <p>ระบบแจ้งเตือนแบบ Mobile App สำหรับเว็บแอปพลิเคชัน</p>
        </header>
        
        <div class="notification-container">
            <div class="card">
                <h2><i class="fas fa-user-check"></i> สถานะการอนุญาต</h2>
                <div class="permission-status">
                    <div style="display: flex; align-items: center;">
                        <div class="status-indicator" id="statusIndicator"></div>
                        <span id="permissionText">กำลังตรวจสอบสิทธิ์...</span>
                    </div>
                    <button class="btn btn-primary" id="requestPermissionBtn">
                        <i class="fas fa-hand-paper"></i> ขออนุญาตแจ้งเตือน
                    </button>
                </div>
                <p>การแจ้งเตือนแบบ Push จำเป็นต้องได้รับการอนุญาตจากผู้ใช้ก่อน ระบบจะสามารถส่งการแจ้งเตือนได้แม้เมื่อปิดเว็บเบราว์เซอร์แล้ว</p>
            </div>
            
            <div class="card">
                <h2><i class="fas fa-cogs"></i> การจัดการแจ้งเตือน</h2>
                <div class="btn-group">
                    <button class="btn btn-success" id="showToastBtn">
                        <i class="fas fa-bell"></i> แจ้งเตือนภายในเว็บ
                    </button>
                    <button class="btn btn-primary" id="sendPushBtn">
                        <i class="fas fa-paper-plane"></i> ส่ง Push Notification
                    </button>
                    <button class="btn btn-secondary" id="playSoundBtn">
                        <i class="fas fa-volume-up"></i> ทดสอบเสียง
                    </button>
                    <button class="btn btn-danger" id="clearNotificationsBtn">
                        <i class="fas fa-trash-alt"></i> ล้างการแจ้งเตือน
                    </button>
                </div>
                <p>ทดสอบระบบแจ้งเตือนต่างๆ ได้ที่นี่ การแจ้งเตือนภายในเว็บจะแสดงที่มุมขวาบน ส่วน Push Notification จะแสดงโดยระบบปฏิบัติการ</p>
            </div>
            
            <div class="card">
                <h2><i class="fas fa-history"></i> การแจ้งเตือนล่าสุด</h2>
                <div class="notification-list" id="notificationList">
                    <!-- แจ้งเตือนจะแสดงที่นี่ -->
                    <div class="notification-item unread">
                        <div class="notification-icon success">
                            <i class="fas fa-check"></i>
                        </div>
                        <div class="notification-content">
                            <h4>ยินดีต้อนรับ!</h4>
                            <p>ระบบแจ้งเตือนพร้อมใช้งานแล้ว</p>
                            <div class="notification-time">เมื่อสักครู่</div>
                        </div>
                    </div>
                </div>
                <p>การแจ้งเตือนทั้งหมดจะถูกบันทึกไว้ที่นี่ คุณสามารถดูประวัติการแจ้งเตือนย้อนหลังได้</p>
            </div>
        </div>
        
        <div class="card">
            <h2><i class="fas fa-info-circle"></i> ข้อมูลเพิ่มเติม</h2>
            <div class="btn-group">
                <button class="btn btn-secondary" id="testInfoBtn">
                    <i class="fas fa-info-circle"></i> แจ้งเตือนข้อมูล
                </button>
                <button class="btn btn-secondary" id="testWarningBtn">
                    <i class="fas fa-exclamation-triangle"></i> แจ้งเตือนเตือน
                </button>
                <button class="btn btn-secondary" id="testErrorBtn">
                    <i class="fas fa-times-circle"></i> แจ้งเตือนข้อผิดพลาด
                </button>
                <button class="btn btn-secondary" id="scheduleNotificationBtn">
                    <i class="fas fa-clock"></i> ตั้งเวลาแจ้งเตือน
                </button>
            </div>
            <p>ระบบนี้ใช้เทคโนโลยีต่อไปนี้: <strong>Notification API</strong>, <strong>Service Workers</strong> (สำหรับ Push Notifications), และ <strong>Web Audio API</strong> (สำหรับเสียง)</p>
        </div>
    </div>
    
    <footer>
        <p>Web App Notification System &copy; 2023 | เหมือนการแจ้งเตือนใน Mobile App</p>
        <p style="font-size: 0.9rem; margin-top: 5px;">ทำงานได้บน Chrome, Firefox, Safari และ Edge รุ่นใหม่</p>
    </footer>

    <!-- Audio สำหรับเสียงแจ้งเตือน -->
    <audio id="notificationSound" preload="auto">
        <source src="https://assets.mixkit.co/sfx/preview/mixkit-correct-answer-tone-2870.mp3" type="audio/mpeg">
    </audio>

    <script>
        // ตรวจสอบสถานะการอนุญาตแจ้งเตือน
        document.addEventListener('DOMContentLoaded', function() {
            checkNotificationPermission();
            registerServiceWorker();
            loadNotifications();
            
            // กำหนด event listeners
            document.getElementById('requestPermissionBtn').addEventListener('click', requestNotificationPermission);
            document.getElementById('showToastBtn').addEventListener('click', showToastNotification);
            document.getElementById('sendPushBtn').addEventListener('click', sendPushNotification);
            document.getElementById('playSoundBtn').addEventListener('click', playNotificationSound);
            document.getElementById('clearNotificationsBtn').addEventListener('click', clearNotifications);
            document.getElementById('testInfoBtn').addEventListener('click', () => showTestNotification('info'));
            document.getElementById('testWarningBtn').addEventListener('click', () => showTestNotification('warning'));
            document.getElementById('testErrorBtn').addEventListener('click', () => showTestNotification('error'));
            document.getElementById('scheduleNotificationBtn').addEventListener('click', scheduleNotification);
        });
        
        // ฟังก์ชันตรวจสอบสถานะการอนุญาต
        function checkNotificationPermission() {
            const statusIndicator = document.getElementById('statusIndicator');
            const permissionText = document.getElementById('permissionText');
            
            if (!('Notification' in window)) {
                statusIndicator.className = 'status-indicator denied';
                permissionText.textContent = 'เบราว์เซอร์นี้ไม่รองรับการแจ้งเตือน';
                document.getElementById('requestPermissionBtn').disabled = true;
                return;
            }
            
            const permission = Notification.permission;
            
            switch(permission) {
                case 'granted':
                    statusIndicator.className = 'status-indicator granted';
                    permissionText.textContent = 'อนุญาตการแจ้งเตือนแล้ว';
                    document.getElementById('requestPermissionBtn').textContent = 'อนุญาตแล้ว';
                    document.getElementById('requestPermissionBtn').disabled = true;
                    break;
                case 'denied':
                    statusIndicator.className = 'status-indicator denied';
                    permissionText.textContent = 'ปฏิเสธการแจ้งเตือนแล้ว';
                    document.getElementById('requestPermissionBtn').textContent = 'ถูกปฏิเสธแล้ว';
                    document.getElementById('requestPermissionBtn').disabled = true;
                    break;
                default:
                    statusIndicator.className = 'status-indicator default';
                    permissionText.textContent = 'ยังไม่ได้ขออนุญาตการแจ้งเตือน';
                    document.getElementById('requestPermissionBtn').disabled = false;
            }
        }
        
        // ขออนุญาตแจ้งเตือนจากผู้ใช้
        function requestNotificationPermission() {
            if (!('Notification' in window)) {
                alert('เบราว์เซอร์นี้ไม่รองรับการแจ้งเตือน');
                return;
            }
            
            Notification.requestPermission().then(permission => {
                checkNotificationPermission();
                
                if (permission === 'granted') {
                    showToastNotification('ได้รับการอนุญาตแล้ว!', 'ตอนนี้คุณจะได้รับการแจ้งเตือนจากเว็บแอปนี้', 'success');
                    playNotificationSound();
                    
                    // ส่งการแจ้งเตือนแรก
                    new Notification('การแจ้งเตือนทำงานแล้ว!', {
                        body: 'ยินดีต้อนรับสู่ระบบแจ้งเตือนของเรา',
                        icon: 'https://cdn-icons-png.flaticon.com/512/733/733585.png',
                        badge: 'https://cdn-icons-png.flaticon.com/512/733/733585.png'
                    });
                }
            });
        }
        
        // แสดง Toast Notification ภายในเว็บ
        function showToastNotification(title, message, type = 'success') {
            if (!title) {
                title = 'การแจ้งเตือนภายในเว็บ';
                message = 'นี่คือตัวอย่างการแจ้งเตือนภายในเว็บแอปพลิเคชัน';
            }
            
            // เพิ่มการแจ้งเตือนลงในรายการ
            addNotificationToList(title, message, type);
            
            // สร้าง Toast Notification
            const toast = document.createElement('div');
            toast.className = `toast-notification ${type}`;
            toast.innerHTML = `
                <div class="notification-icon ${type}">
                    <i class="fas ${getNotificationIcon(type)}"></i>
                </div>
                <div class="notification-content">
                    <h4>${title}</h4>
                    <p>${message}</p>
                </div>
                <button class="close-toast">&times;</button>
            `;
            
            document.body.appendChild(toast);
            
            // ปิด Toast อัตโนมัติหลังจาก 5 วินาที
            setTimeout(() => {
                if (toast.parentNode) {
                    toast.style.opacity = '0';
                    toast.style.transform = 'translateX(100%)';
                    setTimeout(() => {
                        if (toast.parentNode) toast.parentNode.removeChild(toast);
                    }, 500);
                }
            }, 5000);
            
            // ฟังก์ชันปิดเมื่อคลิก X
            toast.querySelector('.close-toast').addEventListener('click', () => {
                toast.style.opacity = '0';
                toast.style.transform = 'translateX(100%)';
                setTimeout(() => {
                    if (toast.parentNode) toast.parentNode.removeChild(toast);
                }, 500);
            });
            
            // เล่นเสียงแจ้งเตือน
            playNotificationSound();
        }
        
        // เพิ่มการแจ้งเตือนลงในรายการ
        function addNotificationToList(title, message, type = 'info') {
            const notificationList = document.getElementById('notificationList');
            const time = new Date().toLocaleTimeString('th-TH', { 
                hour: '2-digit', 
                minute: '2-digit' 
            });
            
            const notificationItem = document.createElement('div');
            notificationItem.className = 'notification-item unread';
            notificationItem.innerHTML = `
                <div class="notification-icon ${type}">
                    <i class="fas ${getNotificationIcon(type)}"></i>
                </div>
                <div class="notification-content">
                    <h4>${title}</h4>
                    <p>${message}</p>
                    <div class="notification-time">${time}</div>
                </div>
            `;
            
            // เพิ่มที่ด้านบนสุดของรายการ
            notificationList.insertBefore(notificationItem, notificationList.firstChild);
            
            // บันทึกลง localStorage
            saveNotificationToStorage(title, message, type, time);
            
            // อัปเดตจำนวนการแจ้งเตือนใน title
            updateNotificationBadge();
        }
        
        // ฟังก์ชันบันทึกการแจ้งเตือนลง localStorage
        function saveNotificationToStorage(title, message, type, time) {
            let notifications = JSON.parse(localStorage.getItem('webapp-notifications') || '[]');
            
            notifications.unshift({
                title,
                message,
                type,
                time,
                timestamp: new Date().getTime()
            });
            
            // จำกัดจำนวนการแจ้งเตือนที่บันทึกไว้
            if (notifications.length > 50) {
                notifications = notifications.slice(0, 50);
            }
            
            localStorage.setItem('webapp-notifications', JSON.stringify(notifications));
        }
        
        // โหลดการแจ้งเตือนจาก localStorage
        function loadNotifications() {
            const notifications = JSON.parse(localStorage.getItem('webapp-notifications') || '[]');
            const notificationList = document.getElementById('notificationList');
            
            // ล้างการแจ้งเตือนเริ่มต้น
            notificationList.innerHTML = '';
            
            if (notifications.length === 0) {
                // ถ้าไม่มีประวัติการแจ้งเตือน
                addNotificationToList('ยินดีต้อนรับ!', 'ระบบแจ้งเตือนพร้อมใช้งานแล้ว', 'success');
                return;
            }
            
            // แสดงการแจ้งเตือนจาก localStorage
            notifications.forEach(notification => {
                const notificationItem = document.createElement('div');
                notificationItem.className = 'notification-item';
                notificationItem.innerHTML = `
                    <div class="notification-icon ${notification.type}">
                        <i class="fas ${getNotificationIcon(notification.type)}"></i>
                    </div>
                    <div class="notification-content">
                        <h4>${notification.title}</h4>
                        <p>${notification.message}</p>
                        <div class="notification-time">${notification.time}</div>
                    </div>
                `;
                
                notificationList.appendChild(notificationItem);
            });
        }
        
        // ล้างการแจ้งเตือนทั้งหมด
        function clearNotifications() {
            if (confirm('คุณต้องการล้างประวัติการแจ้งเตือนทั้งหมดใช่หรือไม่?')) {
                localStorage.removeItem('webapp-notifications');
                const notificationList = document.getElementById('notificationList');
                notificationList.innerHTML = '';
                
                // เพิ่มข้อความว่าไม่มีแจ้งเตือน
                const emptyMessage = document.createElement('div');
                emptyMessage.className = 'notification-item';
                emptyMessage.innerHTML = `
                    <div class="notification-icon info">
                        <i class="fas fa-info-circle"></i>
                    </div>
                    <div class="notification-content">
                        <h4>ไม่มีประวัติการแจ้งเตือน</h4>
                        <p>ยังไม่มีประวัติการแจ้งเตือน</p>
                    </div>
                `;
                
                notificationList.appendChild(emptyMessage);
                
                // แสดง Toast ยืนยัน
                showToastNotification('ล้างการแจ้งเตือนแล้ว', 'ประวัติการแจ้งเตือนทั้งหมดถูกล้างแล้ว', 'info');
            }
        }
        
        // อัปเดต Badge การแจ้งเตือนใน Title
        function updateNotificationBadge() {
            const notifications = JSON.parse(localStorage.getItem('webapp-notifications') || '[]');
            const unreadCount = notifications.filter(n => n.unread).length;
            
            if (unreadCount > 0) {
                document.title = `(${unreadCount}) Web App Notification System`;
            } else {
                document.title = 'Web App Notification System';
            }
        }
        
        // ฟังก์ชันส่ง Push Notification
        function sendPushNotification() {
            if (!('Notification' in window) || Notification.permission !== 'granted') {
                alert('กรุณาอนุญาตการแจ้งเตือนก่อน');
                requestNotificationPermission();
                return;
            }
            
            const title = 'การแจ้งเตือนจากเว็บแอป';
            const options = {
                body: 'นี่คือตัวอย่าง Push Notification ที่ส่งจากเว็บแอปพลิเคชัน',
                icon: 'https://cdn-icons-png.flaticon.com/512/733/733585.png',
                badge: 'https://cdn-icons-png.flaticon.com/512/733/733585.png',
                tag: 'webapp-notification',
                requireInteraction: false,
                actions: [
                    {
                        action: 'open',
                        title: 'เปิดเว็บแอป'
                    },
                    {
                        action: 'close',
                        title: 'ปิด'
                    }
                ]
            };
            
            // ส่งการแจ้งเตือน
            new Notification(title, options);
            
            // เพิ่มในรายการแจ้งเตือน
            addNotificationToList(title, options.body, 'info');
            
            // แสดง Toast ภายในเว็บด้วย
            showToastNotification('ส่ง Push Notification แล้ว!', 'ตรวจสอบการแจ้งเตือนจากระบบปฏิบัติการของคุณ', 'success');
        }
        
        // เล่นเสียงแจ้งเตือน
        function playNotificationSound() {
            const audio = document.getElementById('notificationSound');
            audio.currentTime = 0;
            audio.play().catch(e => console.log('ไม่สามารถเล่นเสียงได้:', e));
        }
        
        // แสดงการแจ้งเตือนทดสอบ
        function showTestNotification(type) {
            const titles = {
                info: ['อัพเดทข้อมูลใหม่', 'มีอัพเดทระบบใหม่พร้อมใช้งาน'],
                warning: ['คำเตือนระบบ', 'ระบบจะปิดปรับปรุงในอีก 10 นาที'],
                error: ['เกิดข้อผิดพลาด', 'ไม่สามารถเชื่อมต่อกับเซิร์ฟเวอร์ได้']
            };
            
            const messages = {
                info: ['โปรดตรวจสอบข้อมูลใหม่ที่อัพเดท', 'เราปรับปรุงระบบเพื่อประสบการณ์ที่ดีขึ้น'],
                warning: ['กรุณาบันทึกงานของคุณก่อนระบบปิด', 'ระบบจะปิดในเวลา 18:00 น.'],
                error: ['โปรดตรวจสอบการเชื่อมต่ออินเทอร์เน็ต', 'ลองอีกครั้งในอีกสักครู่']
            };
            
            const randomIndex = Math.floor(Math.random() * titles[type].length);
            const title = titles[type][randomIndex];
            const message = messages[type][randomIndex];
            
            showToastNotification(title, message, type);
        }
        
        // ตั้งเวลาแจ้งเตือน
        function scheduleNotification() {
            const time = prompt('ตั้งเวลาแจ้งเตือน (นาที จากนี้):', '1');
            
            if (time && !isNaN(time) && time > 0) {
                const minutes = parseInt(time);
                const milliseconds = minutes * 60 * 1000;
                
                setTimeout(() => {
                    if (Notification.permission === 'granted') {
                        new Notification('การแจ้งเตือนตามเวลา', {
                            body: `นี่คือการแจ้งเตือนที่ตั้งไว้ ${minutes} นาทีที่แล้ว`,
                            icon: 'https://cdn-icons-png.flaticon.com/512/733/733585.png'
                        });
                    }
                    
                    showToastNotification('การแจ้งเตือนตามเวลา', `นี่คือการแจ้งเตือนที่ตั้งไว้ ${minutes} นาทีที่แล้ว`, 'info');
                }, milliseconds);
                
                showToastNotification('ตั้งเวลาแล้ว!', `ระบบจะแจ้งเตือนในอีก ${minutes} นาที`, 'success');
            }
        }
        
        // ฟังก์ชันช่วยเหลือ: ดึงไอคอนตามประเภทการแจ้งเตือน
        function getNotificationIcon(type) {
            switch(type) {
                case 'success': return 'fa-check-circle';
                case 'error': return 'fa-times-circle';
                case 'warning': return 'fa-exclamation-triangle';
                default: return 'fa-info-circle';
            }
        }
        
        // ลงทะเบียน Service Worker สำหรับ Push Notifications (จำลอง)
        function registerServiceWorker() {
            if ('serviceWorker' in navigator) {
                navigator.serviceWorker.register('/service-worker.js').catch(error => {
                    console.log('การลงทะเบียน Service Worker ล้มเหลว:', error);
                });
            }
        }
    </script>
</body>
</html>
